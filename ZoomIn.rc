        MENUITEM SEPARATOR
        MENUITEM "&Minimize to Tray",                         ID_MENU_MINIMIZETOTRAY
        MENUITEM "Always on &Top",                            ID_MENU_ALWAYSONTOP
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About ZoomIn++...",                        ID_APP_ABOUT
    END
END


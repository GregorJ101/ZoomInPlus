        MENUITEM "C&opy Command-Line Arguments to Clipboard", ID_COPY_ARGUMENTS_TO_CLIPBOARD
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&Refresh Rate...",                          ID_MENU_REFRESH_RATE
